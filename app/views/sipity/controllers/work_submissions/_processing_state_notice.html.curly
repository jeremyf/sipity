{{#message?}}
  <div class="row">
    <div class="col-xs-12">
      <div class="alert {{notice_dom_class}} alert-dismissible" role="alert">
        <button type="button" class="close" data-dismiss="alert" aria-label="Close"><span aria-hidden="true">&times;</span></button>
        {{message}}
      </div>
    </div>
  </div>
{{/message?}}
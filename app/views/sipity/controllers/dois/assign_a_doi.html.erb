<h2>Assign an existing DOI?</h2>
<%= simple_form_for(model.assign_a_doi_form, url: assign_a_doi_work_doi_path(model.work), method: :post, as: :doi,html: { class: 'assign_a_doi' }) do |f| %>
  <%= f.error_notification %>
  <%= field_set_tag do %>
    <%= f.input :identifier %>
    <%= f.submit %>
  <% end %>
<% end %>

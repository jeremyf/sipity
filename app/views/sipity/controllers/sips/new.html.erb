<%= simple_form_for(model) do |f| %>
  <%= f.error_notification %>
  <%= model.with_form_panel('attributes') do %>
    <%= f.input :title %>
  <% end %>
  <%= model.with_form_panel('work_publication_strategy') do %>
    <%= render 'form_for_work_publication_strategy', { f: f } %>
  <% end %>
  <%= model.with_form_panel('actions') do %>
    <%= f.submit %>
  <% end %>
<% end %>
